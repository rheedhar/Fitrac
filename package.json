{
  "name": "fitrac",
  "version": "1.0.0",
  "description": "An app to track and log workouts",
  "main": "src/main/main.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run clean && npm run compile",
    "start-dev": "nodemon --exec ts-node src/main/main.ts",
    "start": "ts-node src/main/main.ts",
    "test": "jest --passWithNoTests",
    "pre-push": "npm run check-style && npm run test && npm run build",
    "style": "eslint '*/**/*.{ts,js}' --fix && prettier --write '**/*.{ts,js,json}'",
    "check-style": "eslint '*/**/*.{ts,js}' && prettier --check '**/*.{ts,js,json}'",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Fareedat Bello",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.5",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5"
  }
}
